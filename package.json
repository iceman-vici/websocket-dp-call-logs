{
  "name": "websocket-dp-call-logs",
  "version": "1.0.0",
  "description": "Real-time Dialpad call logs via WebSocket - receives webhooks and broadcasts to WebSocket clients",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "NODE_ENV=development node -r dotenv/config server.js dotenv_config_path=.env.development",
    "start:prod": "NODE_ENV=production node -r dotenv/config server.js dotenv_config_path=.env.production",
    "dev": "NODE_ENV=development nodemon -r dotenv/config server.js dotenv_config_path=.env.development",
    "test": "node test-client.js",
    "test:webhook": "node test-webhook.js",
    "test:simple": "node test-rate-limit-retry.js simple",
    "test:load": "node test-rate-limit-retry.js load",
    "test:stress": "node test-rate-limit-retry.js stress",
    "test:load-100": "node test-rate-limit-retry.js load 100 10",
    "test:load-500": "node test-rate-limit-retry.js load 500 20",
    "test:stress-2000": "node test-rate-limit-retry.js stress 2000 100"
  },
  "keywords": [
    "dialpad",
    "websocket",
    "webhook",
    "call-logs",
    "real-time",
    "jwt",
    "socket.io",
    "rate-limiting",
    "retry-logic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "socket.io-client": "^4.6.1",
    "axios": "^1.6.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
