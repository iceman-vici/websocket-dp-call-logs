# Server Configuration
NODE_ENV=development
PORT=3000

# Dialpad Webhook Secret
# Must match the secret configured in your Dialpad webhook
DIALPAD_WEBHOOK_SECRET=your-secret-key-here

# WebSocket Configuration
CORS_ORIGIN=*

# Debug Mode
DEBUG=true

# Rate Limiting
RATE_LIMIT_MAX=1200

# WebSocket URL (for clients)
WEBSOCKET_URL=http://localhost:3000